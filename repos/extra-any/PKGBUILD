# Don't extract anything
noextract=(${source[@]##*/})

_path="/usr/lib/firefox/extensions/"


package_firefox-i18n () {
  pkgdesc="Transition package to split build"
  install=firefox-i18n.install
}

package_firefox-i18n-af() {
  pkgdesc="Afrikaans (South Africa) language pack for Firefox"

  cd "${srcdir}"
  install -Dm644 firefox-i18n-$pkgver-af.xpi "${pkgdir}/$_path/langpack-af@firefox.mozilla.org.xpi"
}

